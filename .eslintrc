{
	"parser" : "babel-eslint",
	"env" : {
		"es6" : true,
		"browser" : true,
		"node" : true
	},
	"rules" : {
		"semi": "off",
		"linebreak-style" :"off",
		"object-curly-spacing" : "off",
		"indent" : "off",
		"no-tabs" : "off",
		"arrow-parens" : "off",
		"require-jsdoc" : "off"
	}, 
	"extends" : [
		"eslint:recommended", "google"
	]
}